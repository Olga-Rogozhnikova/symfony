{% extends 'base.html.twig' %}

{% block title %} Create a new recordgi | {{ parent() }}{% endblock %}

{% block body %}
    <h1>{{ title }}</h1>

    <ul>
        {% for track in tracks %}
            <li>
                {{ track.song }} - {{ track.artist | upper}}
            </li>
        {% endfor %}
    </ul>
{% endblock %}